-- Scripts:

local function ZWIMN_fake_script() -- MusicGUI.New 
	local script = Instance.new('LocalScript', MusicGUI)

	Equalizer = script.Parent.MainBoard.EqualizerBoard;
	Artist = script.Parent.MainBoard.Mainframe.ArtistName;
	Name = script.Parent.MainBoard.Mainframe.SongName;
	MusicStorage = script.Parent.Music;
	Song = script.Parent.MainBoard.EqualizerBoard.Song;
	local u1 = 0;
	function CheckNumber(p1)
		if p1 == u1 then
			return false;
		end;
		u1 = p1;
		return true;
	end;
	local u2 = 0;
	local l__Mainframe__3 = script.Parent.MainBoard.Mainframe;
	function PlaySong()
		wait(3);
		while true do
			if wait() then
	
			else
				break;
			end;
			u2 = math.random(1, 11);
			if CheckNumber(u2) ~= true then
	
			else
				break;
			end;	
		end;
		Song.SoundId = "rbxassetid://" .. MusicStorage["ID" .. u2].Song.Value;
		Artist.Text = MusicStorage["ID" .. u2].Artist.Value;
		Name.Text = MusicStorage["ID" .. u2].SongName.Value;
		l__Mainframe__3:TweenPosition(UDim2.new(0, 0, 0, 75), "Out", "Quart", 1, true);
		Song:Play();
	end;
	Song.Ended:connect(function()
		for v1, v2 in pairs(Equalizer:GetChildren()) do
			if v2:IsA("Frame") then
				v2:TweenSize(UDim2.new(0, 15, 0, 0), "Out", "Quad", 0.1, true);
			end;
		end;
		l__Mainframe__3:TweenPosition(UDim2.new(0, -300, 0, 75), "In", "Quart", 1, true);
		wait(1.5);
		PlaySong();
	end);
	Song.Stopped:connect(function()
		for v3, v4 in pairs(Equalizer:GetChildren()) do
			if v4:IsA("Frame") then
				v4:TweenSize(UDim2.new(0, 15, 0, 0), "Out", "Quad", 1, true);
			end;
		end;
		l__Mainframe__3:TweenPosition(UDim2.new(0, -300, 0, 75), "In", "Quart", 1, true);
		wait(1.5);
		PlaySong();
	end);
	game.Players.LocalPlayer.Chatted:connect(function(p2)
		if string.lower(p2) == "/e skip" then
			Song:Stop();
		end;
	end);
	PlaySong();
	
end
coroutine.wrap(ZWIMN_fake_script)()
local function MRZNVNV_fake_script() -- Mute.real 
	local script = Instance.new('LocalScript', Mute)

	wait();
	local u1 = true;
	local l__Parent__2 = script.Parent;
	function action()
		if u1 then
			l__Parent__2.Parent.Parent.EqualizerBoard.Song.Volume = 0;
			l__Parent__2.Text = "UNMUTE MUSIC";
			u1 = false;
			return;
		end;
		l__Parent__2.Parent.Parent.EqualizerBoard.Song.Volume = 0.1;
		l__Parent__2.Text = "MUTE MUSIC";
		u1 = true;
	end;
	l__Parent__2.MouseButton1Down:connect(action);
	
end
coroutine.wrap(MRZNVNV_fake_script)()


local function AOIDS_fake_script() -- EqualizerBoard.LocalScript 
	local script = Instance.new('LocalScript', EqualizerBoard)

	script.Parent.PlaybackLoudness.Changed:connect(function(p1)
		wait();
		local v1 = {};
		for v2 = 1, 21 do
			v1[v2] = script.Parent:FindFirstChild("Bar" .. v2);
		end;
		for v3 = 21, 2, -1 do
			v1[v3].Size = v1[v3 - 1].Size;
		end;
		v1[1].Size = UDim2.new(0.035, 0, 0, p1 / 7 * -1);
	end);
end
coroutine.wrap(AOIDS_fake_script)()



local function QWERT_fake_script() -- Song.LocalScript
    local script = Instance.new('LocalScript', Song)
    while wait() do
	script.Parent.Parent.PlaybackLoudness.Value = script.Parent.PlaybackLoudness;
end;
end
coroutine.wrap(QWERT_fake_script)(

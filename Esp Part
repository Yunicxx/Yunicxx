--- loadstring
local ESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/goofyclown/carrotEsp/main/esp.lua"))()

-- config
ESP.Players = false
ESP.Boxes = false
ESP.Names = true
ESP.Toggle(true)

-- object
ESP:AddObjectListener(workspace.House, {
    Name = "Bone",
    CustomName = "Bone",
    Color = Color3.fromRGB(255, 255, 255),
    IsEnabled = "Home"
})
ESP.Home = true
